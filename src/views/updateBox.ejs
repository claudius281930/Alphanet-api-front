                    <label for="number">ID:</label>
                    <input type="number" name="id" id="number" required>
                </div>
                <div>
                    <button type="submit">atualizar</button>
                </div>
            </form>
        </div>

        <!-- Footer da pagina -->
        <%- include("./partials/footer") %>
            <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
    </body>

</html>