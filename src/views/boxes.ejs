<!-- Metadados da pagina -->
<%- include("./partials/head" )%>

    <body>
        <%- include("./partials/header") %>


            <p>Caixas</p>


            <ul>
                <% boxesAll.forEach(function(box) { %> <!-- boxesALL é uma const no controlador-->

                    <li>
                        <span class="content-list">ID:</span>
                        <%= box.id %>
                    </li>
                    <li>
                        <span class="content-list">Data de Modificação:</span>
                        <%= box.dateModify %>
                    </li>
                    <li>
                        <span class="content-list">Nome Descritivo:</span>
                        <%= box.nameDescription %>
                    </li>
                    <li>
                        <span class="content-list">Localização:</span>
                        <%= box.locale %>
                    </li>
                    <li>
                        <span class="content-list">Active CTO:</span>
                        <%= box.activeCto %>
                    </li>
                    <li>
                        <span class="content-list">Tecnologia de Rede:</span>
                        <%= box.networkTechnology %>
                    </li>
                    <% }); %>

            </ul>

            <!-- Footer da pagina -->
            <%- include("./partials/footer") %>
    </body>

    </html>